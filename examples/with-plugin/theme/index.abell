{{
  const { globalMeta, contentArray } = Abell;
}}
<!DOCTYPE html>
<html>
  <head>
    <title>{{ globalMeta.siteName }}</title>
  </head>
  <body>
    <section>
      <h2>Content</h2>
      <div data-test="all-slugs">
      {{
        contentArray
          .map(meta => `
          <div>
            <span class="meta-slug">${meta.$slug}</span>
          </div>
          `)
          .join('')
      }}
      </div>
    </section>
  </body>
</html>
