{{
  const { globalMeta, contentArray } = Abell;
}}
<!DOCTYPE html>
<html>
  <head>
    <title>{{ globalMeta.siteName }}</title>
  </head>
  <body>
    <section>
      <h2>Content</h2>
      {{
        contentArray
          .map(meta => `
            <div data-test="meta-slug">${meta.$slug}</div>
          `)
          .join('')
      }}
    </section>
  </body>
</html>
