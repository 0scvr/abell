  <title>{{ globalMeta.siteName }}</title>
</head>
<body>
  <header></header>
  <h1>{{ globalMeta.siteName }}</h1>
  <main>
    {{ # $contentList }}
      <article>
        <a href="{{$slug}}"><h2>{{title}}</h2></a>
