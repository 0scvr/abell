{
  "name": "create-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "node abell-smol/server.js",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "build": "tsc && yarn build:client && yarn build:server",
    "generate": "vite build --outDir dist/static && npm run build:server && node abell-smol/generate.js"
  },
  "devDependencies": {
    "typescript": "^4.4.4",
    "express": "^4.17.1",
    "vite": "^2.7.2"
  }
}